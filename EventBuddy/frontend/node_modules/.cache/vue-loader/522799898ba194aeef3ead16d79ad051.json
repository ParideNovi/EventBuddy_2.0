";\nexport default {\n  name: \"EventActions\",\n  props: {\n    slug: {\n      type: String,\n      required: true\n    }\n  },\n  methods: {\n    async deleteEvent() {\n      let endpoint = `/api/events/${this.slug}/`;\n      try {\n        await apiService(endpoint, \"DELETE\");\n        this.$router.push(\"/\");\n      } catch (err) {\n        console.log(err);\n      }\n    }\n  }\n};\n</script>\n"]}]}